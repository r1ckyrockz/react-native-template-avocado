{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:build": "cd android/ && ./gradlew bundleRelease",
    "ios": "react-native run-ios",
    "ios:install": "cd ios/ && pod install --repo-update",
    "ios:update": "cd ios/ && pod update && cd ../",
    "ios:reinstall": "cd ios && pod deintegrate && pod install --repo-update",
    "ios:clean": "cd ios/ && rm -rf Pods/ && rm Podfile.lock && pod update && pod cache clean --all && pod install",
    "ios:clear": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf src/packages/app/ios/Pods && rm -rf src/packages/app/ios/build && rm -rf src/packages/app/ios/app.xc*",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --cache --format ./src --ext .ts,.tsx,.js",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:types": "tsc --noEmit",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn install",
    "generate:bootsplash": "npx react-native generate-bootsplash src/assets/bootsplash_logo_original.png"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/core": "^5.15.3",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@shopify/restyle": "^1.4.0",
    "i18n-js": "^3.8.0",
    "lodash.memoize": "^4.1.2",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^4.0.2",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^7.6.6",
    "react-native": "0.64.1",
    "react-native-bootsplash": "^3.2.3",
    "react-native-config": "^1.4.2",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.1.0",
    "react-native-paper": "^4.8.1",
    "react-native-reanimated": "^2.1.0",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-text-input-mask": "^3.1.4",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/runtime": "^7.14.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/lodash.memoize": "^4.1.6",
    "@types/react-native": "^0.64.6",
    "@types/react-native-restart": "^0.0.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.27.0",
    "eslint-config-galex": "^2.16.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "metro-react-native-babel-preset": "^0.66.0",
    "pretty-quick": "^3.1.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./testUtils/jestSetupFile.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
